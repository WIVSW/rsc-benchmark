  "dependencies": {
    "@parcel/rsc": "2.15.4",
    "@types/bun": "1.2.20",
    "@types/deno": "2.3.0",
    "@types/express": "5.0.3",
    "@types/node": "24.3.0",
    "@types/parcel-env": "0.0.9",
    "@types/react": "19.1.10",
    "@types/react-dom": "19.1.7",
    "express": "5.1.0",
    "parcel": "2.15.4",
    "process": "0.11.10",
    "react": "19.1.1",
    "react-dom": "19.1.1"
