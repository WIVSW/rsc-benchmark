{
  "name": "app",
  "version": "1.0.0",
  "server": "dist/server.js",
  "source": "src/server.tsx",
  "targets": {
    "react": {
      "source": "src/react.ts",
      "distDir": "dist",
      "engines": {
        "node": ">= 22"
      },
      "includeNodeModules": {
        "react": true,
        "react-dom/server": true
      }
    },
    "server": {
      "distDir": "dist",
      "context": "react-server",
      "engines": {
        "node": ">= 22"
      },
      "includeNodeModules": {
        "express": false
      }
    }
  },
  "scripts": {
    "start": "node dist/server.js",
    "build": "rm -rf dist && parcel build"
  },
